{
  "name": "@tinymce/tinymce-jquery",
  "version": "2.1.1-rc",
  "description": "Official TinyMCE integration for jQuery",
  "main": "dist/tinymce-jquery.js",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.txt"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rimraf lib dist scratch",
    "test": "bedrock-auto -b chrome-headless -d src/test/ts",
    "test-manual": "bedrock -d src/test/ts",
    "build": "yarn run clean && tsc -p ./tsconfig.json && rollup -c rollup.config.js",
    "lint": "eslint src",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "deploy-storybook": "yarn storybook-to-ghpages --source-branch=main"
  },
  "repository": "https://github.com/tinymce/tinymce-jquery",
  "author": "Tiny Technologies",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.28.0",
    "@ephox/agar": "^7.4.1",
    "@ephox/bedrock-client": "^15.0.0",
    "@ephox/bedrock-server": "^15.0.2",
    "@ephox/katamari": "^9.1.5",
    "@ephox/mcagar": "^8.3.2",
    "@ephox/sand": "^6.0.9",
    "@ephox/sugar": "^9.2.1",
    "@ephox/swag": "^4.6.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@storybook/addon-docs": "^8",
    "@storybook/addon-links": "^8",
    "@storybook/cli": "^8",
    "@storybook/html-webpack5": "^8.6.14",
    "@storybook/storybook-deployer": "^2.8.16",
    "@tinymce/beehive-flow": "^0.19.0",
    "@tinymce/eslint-plugin": "^2.2.1",
    "@types/jquery": "^3.5.16",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "babel-loader": "^8.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "storybook": "^8",
    "tinymce": "^6.4.0",
    "tslib": "^2.5.0",
    "typescript": "~4.8.3",
    "webpack": "^5.9.0"
  },
  "dependencies": {
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6"
  }
}
